{
  "name": "zaki-night",
  "version": "0.0.5",
  "description": "Zaki é um sistema de alto desempenho desenvolvido com JavaScript para criar um bot para WhatsApp, suporte para criar qualquer interação, como atendimento ao cliente, envio de mídia, reconhecimento de sentenças baseado em inteligência artificial e todos os tipos de arquitetura de design para WhatsApp. ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:counter": "cd src/lib/counter/ && gulp",
    "build:docs": "typedoc && git add docs/*",
    "build:jsQR": "cd src/lib/jsQR/ && gulp",
    "build:middleware": "cd src/lib/middleware/ && webpack",
    "build:venom": "tsc",
    "build:wapi": "cd src/lib/wapi/ && webpack",
    "build": "npm run build:wapi && npm run build:middleware && npm run build:jsQR && npm run build:counter && npm run build:venom",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean": "shx rm -rf session dist",
    "commit": "cz",
    "fix:doc-theme": "shx cp ./node_modules/typedoc-default-themes/bin/default/partials/type.hbs ./node_modules/typedoc-plugin-pages/dist/theme/v2/partials/type.hbs",
    "generate-api-docs": "npm run fix:doc-theme && typedoc",
    "lint:js": "npx eslint -c .eslintrc.js --ext .js src",
    "lint:ts": "npx eslint -c .eslintrc.js --ext .ts src",
    "lint": "npm run lint:ts && npm run lint:js",
    "lint:fix": "npx eslint src --fix",
    "prepublish": "npm run clean && npm run build",
    "release": "release-it",
    "start": "npm run build:venom & tsc app.ts && node app.js",
    "test": "echo \"No tests yet\"",
    "watch": "concurrently \"tsc -w\" \"nodemon dist/index.js\""
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeronightk/zaki_night.git"
  },
  "keywords": [
    "whatsapp",
    "javascript",
    "bot",
    "chat bot",
    "bot",
    "typescript",
    "automatization",
    "puppeteer"
  ],
  "author": "Zeronight",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeronightk/zaki_night/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    ".env",
    "dist/*",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "SECURITY.md"
  ],
  "homepage": "https://github.com/zeronightk/zaki_night",
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-angular": "^16.0.0",
    "@types/atob": "^2.1.2",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^17.0.8",
    "@types/puppeteer": "^5.4.0",
    "@types/sharp": "^0.29.5",
    "@types/ws": "^8.2.0",
    "concurrently": "^7.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "copy-webpack-plugin": "^10.2.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.4.0",
    "gulp": "^4.0.2",
    "husky": "^7.0.4",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "pretty-quick": "^3.1.3",
    "release-it": "^14.12.4",
    "shx": "^0.3.4",
    "ts-loader": "^9.2.6",
    "typedoc": "^0.21.2",
    "typedoc-plugin-pages": "^1.1.0",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "atob": "^2.1.2",
    "axios": "^0.21.4",
    "boxen": "^5.1.1",
    "catch-exit": "^1.1.0",
    "chalk": "^4.0.0",
    "chrome-launcher": "^0.15.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^11.0.0",
    "futoin-hkdf": "^1.4.3",
    "latest-version": "^5.1.0",
    "mime-types": "^2.1.33",
    "ora": "^5.4.1",
    "puppeteer": "^13.2.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "qrcode-terminal": "^0.12.0",
    "sanitize-filename": "^1.6.3",
    "sharp": "^0.29.1",
    "spinnies": "^0.5.1",
    "tree-kill": "^1.2.2",
    "typedoc-default-themes": "^0.12.10",
    "winston": "^3.4.0",
    "ws": "^8.4.2",
    "yoo-hoo": "^1.2.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.2.1"
  }
}
